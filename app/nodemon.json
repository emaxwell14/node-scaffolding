{
  "restartable": "rs",
  "events": {
    "restart": "echo 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "watch": [
    "node_modules/",
    "src/"
  ],
  "ignore": [
    ".git",
    ".tmp/",
    "buildscripts"
  ],
  "env": {
    "LOG_LEVEL": "debug",
    "JWT_SECRET": "node-scaffolding-encrypt"
  },
  "options": {
    "extensions": [
      "js"
    ],
    "exec": "node"
  }
}
